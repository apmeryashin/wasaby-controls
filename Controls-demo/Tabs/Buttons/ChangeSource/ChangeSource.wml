<div class="root {{(_options.theme.indexOf('default') !== 0 || _options.theme.indexOf('light') !== 0) ? ' root__dark_background'}}">
    <div class="separator">
        <span>Style="primary", change source after click</span>
        <div class="clickmebtn" on:click="_setSource()">click me</div>
    </div>
    <div class="demo-TabButtons ControlsDemo-Tabs__clickable">
        <Controls.tabs:Buttons
                bind:selectedKey='SelectedKey'
                keyProperty="id"
                name="lazy"
                source = "{{_source}}"
        />

    </div>
</div>

<div class="ws-flexbox">
    <div class="controlsDemo__wrapper controlsDemo_zen_dark">
        <div class="controlsDemo__cell controlsDemo_fixedWidth500">
            <div class="controls-text-label">Dark</div>
            <Controls.themes:ZenWrapper brightness="dark" complementaryColor="119, 195, 216" dominantColor="(49, 49, 49)">
                <ws:partial template="wml!Controls-demo/themes/ZenWrapper/resources/themedContent" />
            </Controls.themes:ZenWrapper>
        </div>
    </div>
    <div class="controlsDemo__wrapper controlsDemo_zen_light">
        <div class="controlsDemo__cell controlsDemo_fixedWidth500">
            <div class="controls-text-label">Light</div>
            <Controls.themes:ZenWrapper brightness="light" complementaryColor="178, 35, 35" dominantColor="(255, 255, 255)">
                <ws:partial template="wml!Controls-demo/themes/ZenWrapper/resources/themedContent" />
            </Controls.themes:ZenWrapper>
        </div>

    </div>
    <div class="controlsDemo__wrapper controlsDemo_fixedWidth500">
        <div class="controlsDemo__cell">
            <div class="controls-text-label">Original</div>
            <ws:partial template="wml!Controls-demo/themes/ZenWrapper/resources/themedContent"/>
        </div>

    </div>

<div class="controlsDemo__wrapper controlDemo_Tile_marker">
<Controls.tileOld:View
        tileMode="dynamic"
        displayProperty="title"
        imageProperty="image"
        keyProperty="id"
        markerVisibility="hidden"
        source="{{_viewSource}}">
        <ws:itemTemplate>
                <ws:partial
                        template="Controls/tileOld:ItemTemplate"
                        hasTitle="{{itemTemplate.itemData.item.isDocument}}"/>
        </ws:itemTemplate>
</Controls.tileOld:View>
</div>

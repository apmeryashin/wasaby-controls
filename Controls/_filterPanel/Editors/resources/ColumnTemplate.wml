<ws:partial template="Controls/grid:ColumnTemplate" scope="{{_options}}" attr:class="controls-EditorList__columns">
    <ws:contentTemplate>
        <div class="{{itemData.column.selected && itemData.column.selected.length && !itemData.column.selected.includes(itemData.item[itemData.column.displayProperty]) ?
                    'controls-EditorList__column_unselected-title_theme-' + theme}}
                    controls-EditorList__column_theme-{{theme}}">
            {{itemData.item[itemData.column.displayProperty]}}
        </div>
    </ws:contentTemplate>
</ws:partial>

<span>
    <ws:for data="index, text in _textLines">
        <ws:if data="{{ index > 0 }}">
            <br/>
        </ws:if>
        <ws:if data="{{text !== ''}}">
            <Controls._decorator.WrapURLs attr:class="controls-MultilineText-display_wrapper" value="{{text}}"/>
        </ws:if>
    </ws:for>
</span>

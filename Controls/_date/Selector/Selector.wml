<Controls._date.LinkView
        name="linkView"
        scope="{{_options}}"
        value="{{ _options.value }}"
        on:keyDown="_keyDownHandler()"
        on:linkClick="openPopup()"
        on:valueChanged="_rangeChangedHandler()"
        on:mouseenter="_mouseEnterHandler()"
        on:mouseleave="_mouseLeaveHandler()"
        on:resetButtonClick="_resetButtonClickHandler()"
        attr:class="controls-DateLink controls-DateLink__view"
/>

<Controls.Container.Async templateName="Controls/dateRange:Input">
    <ws:partial template="{{content.resolvedTemplate}}"
                scope="{{_options}}"
                bind:startValue="_startDate"
                bind:endValue="_endDate"
                on:inputCompleted="_handleInputCompleted()"
                attr:class="controls-PropertyGrid__editor_dateRange_control"/>
</Controls.Container.Async>

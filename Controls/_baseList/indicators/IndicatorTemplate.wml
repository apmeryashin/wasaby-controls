<div class="{{ item.getClasses() }}"
     style="{{ item.getStyles() }}"
     data-qa="{{ item.listElementName }}_state_{{ item.getState() }}_position_{{ item.getPosition() }}"
     vdom-ignore="{{ item.isDrawingIndicator() ? true : false }}"
>
    <ws:partial if="{{ item.getContentTemplate() }}" template="{{ item.getContentTemplate() }}"/>
</div>

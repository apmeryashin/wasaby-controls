<div on:click="_clickHandler()"
     class="controls_toggle_theme-{{_options.theme}} controls-MoreButton">
    <div class="controls-MoreButton-{{_options.readOnly ? 'readonly' : 'textContainer'}}
                controls-MoreButton-container controls-MoreButton-container-size-{{_options.fontSize}}
                {{(_options.contrastBackground) ? 'controls-MoreButton-contrast' : 'controls-MoreButton-same'}}">
        <span class="controls-text-label controls-fontsize-{{_options.fontSize}}">{[Еще]} {{_options.moreCount}}</span>
    </div>
</div>

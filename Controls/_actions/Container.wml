<ws:partial
        template="{{_options.content}}"
        name="toolbar"
        scope="{{_options.toolbarOptions}}"
        items="{{_toolbarItems}}"
        menuSource="{{_menuSource}}"
        keyProperty="id"
        parentProperty="parent"
        itemTemplateProperty="template"
        nodeProperty="parent@"
        on:itemClick="_toolbarItemClick()"/>


<tr>
    <td colspan="{{ colspan }}">
        <div style="{{ item.getStyles() }}" class="{{ item.getClasses() }}">
            <div name="{{item.isTopIndicator() ? 'topIndicator' : item.isBottomIndicator() ? 'bottomIndicator' }}"
                 data-qa="{{ item.listInstanceName }}_state_{{ item.getState() }}_position_{{ item.getPosition() }}">
                <ws:partial if="{{ item.getContentTemplate() }}" template="{{ item.getContentTemplate() }}"/>
            </div>
        </div>
    </td>
</tr>

<tr>
    <td colspan="{{ colspan }}">
        <div style="{{ item.getStyles() }}" class="{{ item.getClasses() }}">
            <div data-qa="{{ item.listElementName }}_state_{{ item.getState() }}_position_{{ item.getPosition() }}">
                <ws:partial if="{{ item.getContentTemplate() }}" template="{{ item.getContentTemplate() }}"/>
            </div>
        </div>
    </td>
</tr>

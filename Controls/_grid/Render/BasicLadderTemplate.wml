<div class="{{ gridColumn.getTextOverflowClasses() }}">
    <ws:partial template="{{gridColumn.getCellContentRender(true)}}"
                attr:title="{{ gridColumn.getTextOverflowTitle() }}"
                if="{{ gridColumn.LadderContentCell && gridColumn.shouldDrawLadderContent(gridColumn.getDisplayProperty()) }}"
                attr:class="{{ gridColumn.getLadderWrapperClasses(gridColumn.getDisplayProperty()) }}"
                value="{{gridColumn.getDefaultDisplayValue()}}"
                searchValue="{{gridColumn.getSearchValue()}}"
                scope="{{gridColumn.config}}"/>
</div>

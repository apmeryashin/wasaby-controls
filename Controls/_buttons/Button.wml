<ws:partial template="wml!Controls/_buttons/ButtonBase"
            _viewMode="{{_viewMode}}"
            _options="{{_options}}"
            _buttonStyle="{{_options.translucent ? 'translucent' : _buttonStyle}}"
            _contrastBackground="{{_contrastBackground}}"
            _height="{{_height}}"
            _fontSize="{{_fontSize}}"
            _icon="{{_icon}}"
            _iconSize="{{_iconSize}}"
            _hoverIcon="{{_hoverIcon}}"
            _caption="{{_caption}}"
            _fontColorStyle="{{_fontColorStyle}}"
            _hasIcon="{{_hasIcon}}"
            _stringCaption="{{_stringCaption}}"
            _iconStyle="{{_iconStyle}}"
            _captionPosition="{{_captionPosition}}"
            _isSVGIcon="{{_isSVGIcon}}"
            _textAlign="{{_textAlign}}"
            _translucent="{{_options.translucent}}"
            _tooltip="{{_tooltip}}"
            attr:class="{{_isWorkByKeyboard() ? 'controls-focused-item'}}"
            on:focusin="_focusInHandler()"
            on:focusout="_focusOutHandler()"
            on:click="_clickHandler()"
            on:mouseenter="_onMouseEnterHandler()"
            on:keypress="_keyUpHandler()"/>
